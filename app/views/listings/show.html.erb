<%= render('shared/header') %>

<div class="row" style="margin-top: 10px;">
  <div class="large-9 columns">
    <div class="row">
      <div class="large-12 columns">
        <div class="listingCoverImage">
          <%= image_tag(@listing.image.cover.url) %>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="large-12 columns">
        <div class="listingShowBody">
          <div class="row">
            <div class="large-6 columns">
              <h4>Listing Title</h4>
            </div>

            <div class="large-6 columns">
              <span>Large-6</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="large-3 columns">
    <div class="row">
      <div class="large-12 columns">
        <div class="listingShortInformation">
          <span>X</span>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="large-12 columns" style="width: 100%; height: 200px; background-color: green;">
        <span>X</span>
      </div>
    </div>
  </div>
</div>
